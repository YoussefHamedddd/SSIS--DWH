    <DTS:Executable
      DTS:refId="Package\Clean and Load to DataWarehouse"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{322B6D36-1410-4ACE-AE16-82A9DF5D82A4}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Clean and Load to DataWarehouse"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2025; © 2025 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{410C2AF4-185A-49E0-8961-15F3F4D8F230}"
          SQLTask:SqlStatementSource="&#xA;TRUNCATE TABLE Fact_Sales;&#xA;DELETE FROM Dim_Customer;&#xA;DELETE FROM Dim_Product;&#xA;DBCC CHECKIDENT ('Dim_Customer', RESEED, 0);&#xA;DBCC CHECKIDENT ('Dim_Product', RESEED, 0);&#xA;&#xA;&#xA;INSERT INTO Dim_Customer (CustomerName, City)&#xA;SELECT DISTINCT &#xA;   &#xA;    REPLACE(LTRIM(RTRIM(CustomerName)), '&quot;', '') AS CustomerName,&#xA;    CASE &#xA;        WHEN City IS NULL OR LTRIM(RTRIM(City)) = '' OR City = '&quot;' OR City = '.' &#xA;        THEN 'NOT FOUND' &#xA;        ELSE REPLACE(LTRIM(RTRIM(City)), '&quot;', '') &#xA;    END AS City&#xA;FROM Staging_Sales&#xA;WHERE CustomerName IS NOT NULL;&#xA;&#xA;&#xA;&#xA;INSERT INTO Dim_Product (ProductID)&#xA;SELECT DISTINCT &#xA;    lower(LTRIM(RTRIM(ProductCode)))&#xA;FROM Staging_Sales&#xA;WHERE ProductCode IS NOT NULL;&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;INSERT INTO Fact_Sales (ProductKey, CustomerKey, OrderNumber, SalesAmount, Quantity, DateKey, TotalSales)&#xA;SELECT &#xA;    p.ProductKey,&#xA;    c.CustomerKey,&#xA;    s.OrderNumber,&#xA;    TRY_CAST(REPLACE(s.PriceEach, '&quot;', '') AS DECIMAL(18,2)), &#xA;    TRY_CAST(REPLACE(s.QuantityOrdered, '&quot;', '') AS INT),&#xA;    CASE &#xA;        WHEN ISDATE(REPLACE(s.OrderDate, '&quot;', '')) = 1 &#xA;        THEN (YEAR(CAST(REPLACE(s.OrderDate, '&quot;', '') AS DATETIME)) * 10000) + &#xA;             (MONTH(CAST(REPLACE(s.OrderDate, '&quot;', '') AS DATETIME)) * 100) + &#xA;              DAY(CAST(REPLACE(s.OrderDate, '&quot;', '') AS DATETIME))&#xA;        ELSE NULL &#xA;    END,&#xA;    TRY_CAST(REPLACE(s.PriceEach, '&quot;', '') AS DECIMAL(18,2)) * TRY_CAST(REPLACE(s.QuantityOrdered, '&quot;', '') AS INT)&#xA;FROM Staging_Sales s&#xA;JOIN Dim_Customer c ON REPLACE(s.CustomerName, '&quot;', '') = c.CustomerName&#xA;JOIN Dim_Product p ON REPLACE(s.ProductCode, '&quot;', '') = p.ProductID;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
